{
    "name": "alingai/pro",
    "description": "AlingAi Pro - 先进的AI应用框架",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "AlingAi Team",
            "email": "team@alingai.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.3",
        "vlucas/phpdotenv": "^5.3",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/console": "^5.3",
        "symfony/http-foundation": "^5.3",
        "doctrine/dbal": "^3.1",
        "nesbot/carbon": "^2.53",
        "ramsey/uuid": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^0.12",
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs": "phpcs",
        "stan": "phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}